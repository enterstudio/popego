/* 	cursor:move; */
/* } */

.tag_hits_lowest {
	font-size:12px;
}

.tag_hits_low {
	font-size:12px;
}

.tag_hits_top {
	font-size:16px;
}

.tag_hits_highest {
	font-size:20px;
}

.tag_cloud {
	text-align:center;
	line-height:25px;
	position: relative;
}

.trash_panel {
	color:#ddd;
	text-align:center;
	height:91px;
	background:url(/images/website/trash_panel.png) no-repeat top center;
	padding:10px 20px;
	font-size:11px;
	line-height:14px;
	
	/* IE 6&7 */
	_background:none;
	_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/website/trash_panel.png', sizingMethod='scale');
	*height:81px;
	*margin-left:5px;
}

.trash_panel a {
	color:#fff;
}

.trash_panel img,
.trash_panel p {
	margin:0;
	padding:0 10px;
}

.arrow_up,
.arrow_down {
	background:url(../../images/website/box_arrow_up.png) top center no-repeat;
	margin:auto;
	height:18px;
	width:30px;

	/* IE 6 */
	_background:none;
	_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/website/box_arrow_up.png', sizingMethod='crop');

}

.arrow_down {
	background:url(../../images/website/box_arrow_down.png) bottom center no-repeat;
	margin-top:-8px;
	
	/* IE 6 */
	_background:none;
	_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/website/box_arrow_down.png', sizingMethod='crop');
	
}

.tag_delete {
	font-weight:bolder;
	background-color:#c0c0c0;
	height:16px;
	width:24px;
	padding:4px 0px; /*6px 0px 4px;*/
	_position:relative;
	_padding:3px 0px 1px;
	_top:3px;
	_margin-bottom:10px;

	display:inline;
	color:#fff;
	
	/** IE hover **/
    _over: expression(onmouseover=new Function("this.style.cursor='hand'; this.style.background='#5f5f5f'"));
    _out: expression(onmouseout=new Function("this.style.cursor=''; this.style.background='#c0c0c0'"));
}

.tag_delete:hover {
	cursor:pointer;
	background-color:#5f5f5f;
}

/********* Category Selector ************/

.databox {
	padding:0px;
	word-spacing:1px;
	clear:both;
	display:table;
	width:100%;
	/*max-width:410px;*/
	max-height:500px;
	font-size:12px;
	margin-bottom:10px;
}

.category_reduced,
.category_selected,
.category {
	height:22px;
	line-height:18px;
	padding:1px 6px 0px 6px;
	margin:2px;
	
	float:left;
	white-space:nowrap;	
}

.category_selected {
	/*border-left:1px solid #aaaaaa;
	border-top:1px solid #aaaaaa;*/
	padding:2px 7px 1px 7px;
	color:#ffffff;
}

.category {
	/*background-color:#f3f3f3;
	border-right:2px solid #e5e5e5;
	border-bottom:2px solid #e5e5e5;*/
	
	background:#f3f3f3;
	border:1px #d3d3d3 solid;
	border-bottom:1px solid #d3d3d3;
	border-right:1px solid #d3d3d3;	
}

/*
.category:hover {
	background-color:#f9f9f9;
	border-right:2px solid #f3f3f3;
	border-bottom:2px solid #f3f3f3;
}*/

.category:hover strong {
	text-decoration:underline;
	cursor:pointer;
}

/** IE hover **/
.category strong {
    _over: expression(onmouseover=new Function("this.style.textDecorationUnderline='true'; this.style.cursor='hand'"));
    _out: expression(onmouseout=new Function("this.style.textDecorationUnderline=''; this.style.cursor=''"));
}

.suggest_link {
	text-align:center;
	font-size:12px;
	margin-top:20px;
	clear:both;
}

.message {
	max-width:215px;
	text-align:center;
	margin:40px 0;
}


/********* Footer Stuff ************/

.final_links {
	float:right;
}

.website_menu {
	float:left;
	margin:12px 0;
}

.final_links ,
.final_links a {
	padding-left:15px;
	text-align:right;	
	font-size:11px;
	margin:25px 0;
}

.final_links a {
	border-left-style:solid;
	border-left-color:#909090;
	border-left-width:1px;
	margin:0 10px;	
}

.footer {
	background:url(../../images/website/footer_background.png) no-repeat top center;
	width:950px;
	height:24px;
	margin:auto;
	margin-bottom:30px;
	
	/* IE 6 */
	_background:none;
	_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/website/footer_background.png', sizingMethod='scale');
		
}

.footer_separator {
	background:url(../../images/website/footer_bar.png) repeat-x top center;
	width:950px;
	height:11px;

	margin:20px -40px;
	clear:both;	
	
	/* IE 6 */
	_background:none;
	_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/website/footer_bar.png', sizingMethod='scale');
		
}

/********* Embedder ****************/
.embedder {
	max-width:180px;
}

.embedder textarea {
	width:180px;
	height:60px;
	margin:6px 0;
}

/********* Tool Tip Box ************/

.tool_tip_box {
	background:#ffffe1;
	border:1px solid #c2c2c2;
	position:absolute;
	padding:10px;
	opacity:0.95;
	z-index:9999;
	max-width:340px;
}

.tool_tip_box h1 {
	font-size:16px;	
}

.tool_tip_box .databox {
	font-size:11px;	
	margin-top:10px;
	margin-bottom:10px;
}

.tool_tip_box img {
	float:left;
	margin:0px 7px 6px 0;	
}


/******* COMING SOON ***************************/

.coming_soon_wrapper {
	background:#fff;
	z-index:9999;
}

.coming_soon_wrapper a:hover {
	text-decoration:none;
}

.coming_soon_wrapper:hover {
	cursor:pointer;	
}

.coming_soon_pic {
	margin:0 auto;
	position:absolute;
}

.special_button {
	background:url(/images/website/special_button.png) top left no-repeat;
	width:240px;
	height:44px;
	text-align:center;
	padding-top:8px;
	
	/* IE 6 */
	_background:none;
	_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/website/special_button.png', sizingMethod='image');
}


.special_button a {
	color:#fff;
}

/******* Dialog Box ***************************/
#overlay {
	background:#000;
	width:100%;
	height:100%;
	position:absolute;
	top:0;
	left:0;
	opacity:0.6;
	z-index:9998;
}

#dialogs {
	position: absolute;
	left: 0;
	width: 100%;
	text-align: center;
	line-height: 0;
	margin: 0 auto;
	z-index: 9999;
}

/******* Floating Form **************/

.floating_form {
	position:absolute;
	background:url(/images/website/floating_form_background.png) top left no-repeat;
	width:480px;
	height:265px;
	padding:35px 40px 40px 40px;
	line-height:22px;	
	margin:0 auto;
	z-index: 9999;
	
	/* IE */
	_background:none;
	_background-color:white;
}

.closer_white {
	background:url(/images/website/close_white_idle.png) top left no-repeat;
	width:24px;
	height:24px;
	position:absolute;
	right:0;
	top:0;
	margin:25px 25px 0 0;
	
	/* IE 6 */
	_background:none;
	_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/website/close_white_idle.png', sizingMethod='image');
}

.closer_white:hover {
	background:url(/images/website/close_white_hover.png) top left no-repeat;
	cursor:pointer;
}
