/*******************************
 ** Widget Theme: COLOR
 ** Base Color: #COLOR
 ******************************/
#popego.widget {
	background:url(/images/widget/widget_background_COLOR.png) top left no-repeat;
	/* IE */
	_background: none;
	_background-image: expression(
        this.style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='" + Popego.Env.baseUrl + "/images/widget/widget_background_COLOR.png', sizingMethod='scale')",
		this.style.width = "143px",
		this.style.height = "235px"
    );
}