/*******************************
 ** Widget Theme: 3d3d3d 
 ** Base Color: #3d3d3d
 ******************************/
.frame {
	background:url(/images/widget/widget_frame_3d3d3d.png) top left no-repeat;
	/* IE */
	_background: none;
	_background-image: expression(
        this.style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/widget/widget_frame_3d3d3d.png', sizingMethod='scale')",
		this.style.height = "147px"
    );
}

.fact,
.user_login,
.user_encounter {
	background: #3d3d3d;
}