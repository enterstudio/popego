/*******************************
 ** Widget Theme: ac20ac 
 ** Base Color: #ac20ac
 ******************************/
.frame {
	background:url(/images/widget/widget_frame_ac20ac.png) top left no-repeat;
	/* IE */
	_background: none;
	_background-image: expression(
        this.style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/widget/widget_frame_ac20ac.png', sizingMethod='scale')",
		this.style.height = "147px"
    );
}

.fact,
.user_login,
.user_encounter {
	background: #ac20ac;
}